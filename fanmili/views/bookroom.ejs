<%- include("header.ejs",{"title":title}) %>

<h1 class="page_title">会议室预订</h1>

<div class="weui_cells weui_cells_form">
    <div class="weui_cell">
        <div class="weui_cell_hd"><label for="" class="weui_label">日期</label></div>
        <div class="weui_cell_bd weui_cell_primary">
            <input class="weui_input" type="date" value="">
        </div>
    </div>

    <div class="weui_cell weui_cell_select weui_select_after">
        <div class="weui_cell_hd">
            <label for="" class="weui_label">楼层</label>
        </div>
        <div class="weui_cell_bd weui_cell_primary">
            <select class="weui_select" name="select2">
                <option value="24">24楼</option>
            </select>
        </div>
    </div>
    <div class="weui_cell weui_cell_switch">
        <div class="weui_cell_hd weui_cell_primary">只显示可预订</div>
        <div class="weui_cell_ft">
            <input class="weui_switch" type="checkbox">
        </div>
    </div>
</div>


<div class="page-list">


    <div class="weui_cells_title">带图标、说明的列表项</div>
